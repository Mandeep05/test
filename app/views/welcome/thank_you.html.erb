<style type="text/css">
  body{font-family: "Helvetica", Arial, sans-serif !important;}
  #bodyTable {height:100% !important; margin:0; padding:0; width:100% !important;}
  #templateContainer { max-width: 720px; width: 100%; }
  #templatePreheader{ padding: 0 30px;}
  .main-container{margin: 0 auto;background: #ffffff;}

  @media only screen and (max-width: 480px) {
    #templateContainer { max-width: 600px !important; width: 100% !important; }
    #templatePreheader{ padding: 0px 30px !important; }
    .main-container{
      width: 100% !important;
      border: 0 !important;
    }

    .desk-img{
      width: 100% !important;
    }

    .academy-img-cell{
      display: none !important;
    }

    .container, .content{
      max-width: 100% !important;
    }

    .centered-cell{
      text-align: center !important;
    }

    .calendar-cell{
      display: none !important;
    }

    .class-info-cell{
      padding: 0 !important;
    }

    a.btn{
      display: inline-block !important;
      padding: 10px 50px !important;
    }

    .btn-holder {
      text-align: center !important;
    }

    .teacher-info-table{
      display: block !important;
      padding: 15px 18px!important; 
    }

    .teacher-info-cell{
      display: block!important;
      text-align: center;
      }

    .teacher-info-cell p, #name-cell{
      text-align: left !important;
    }

    .teacher-info-cell h4 {
      font-size: 16px !important;
      font-weight: bold !important;
      padding-top: 10px !important;
    }

    #teacher-pic{
      display: inline-block !important;
    }

    #teacher-bio {
      padding-left: 0 !important;
    }

    .footer-img-holder, .about-header-cell, .about-table-cell{
      display: block !important;
      text-align: center !important;
    }

    .about-company-table{
      display: none !important;
    }

    .hidden {
      overflow: auto !important; 
      width: auto !important;  
      height: auto !important;
      max-height: inherit !important; 
    }

    .footer, .teacher-desktop, .desktop-desk, .hide-for-mobile {
      display: none !important;
    }

    td[class=headercell] img {
      height:auto !important;
      width:100% !important;
    }

    .none {
      display: none !important;
    }

    .unsubscribe-info {
      padding: 0 5px !important;
    }

    .mobile {
      display: block !important;
    }
    .green-btn {
      background-color: #60B86A;
      -webkit-box-shadow: 0 5px 0px 0px #44884D !important;
      -moz-box-shadow: 0 5px 0px 0px #44884D !important;
      box-shadow: 0 5px 0px 0px #44884D !important;
    }
    .blue_btn {
      background-color: #407BCF !important;
      -webkit-box-shadow: 0 5px 0px 0px #2E5997 !important;
      -moz-box-shadow: 0 5px 0px 0px #2E5997 !important;
      box-shadow: 0 5px 0px 0px #2E5997 !important; 
    }

    .main-cell{
      padding-bottom: 0 !important;
    }
  }
</style>
